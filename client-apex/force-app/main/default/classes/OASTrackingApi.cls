        List<OAS.Param> form = new List<OAS.Param>();

        return (List<OASLegResponse>) client.invoke(
            'GET', '/tracking/searchByBillable', '',
            query, form,
            new Map<String, Object>(),
            new Map<String, Object>(),
            new List<String>{ '*/*' },
            new List<String>(),
            new List<String>(),
            List<OASLegResponse>.class
        );
    }
